{
  "name": "sburba/strict-json",
  "description": "A strict and basic json parser",
  "type": "library",
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "infection/infection": "^0.12.2 || ^0.13.0",
    "friendsofphp/php-cs-fixer": "^2.14",
    "couscous/couscous": "^1.7",
    "phpbench/phpbench": "^0.16.9",
    "phpstan/phpstan": "^0.11.5",
    "phpstan/phpstan-phpunit": "^0.11.0",
    "phpstan/phpstan-strict-rules": "^0.11.0"
  },
  "autoload": {
    "psr-4": {
      "Burba\\StrictJson\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Burba\\StrictJson\\": "test/"
    }
  },
  "require": {
    "ext-json": "*"
  }
}
